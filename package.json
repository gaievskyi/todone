{
  "name": "todone",
  "private": true,
  "description": "Animated todo app as a part of Mobile Apps lab.",
  "version": "1.0.0",
  "author": {
    "name": "Daniel Gajewski"
  },
  "license": "MIT",
  "keywords": [
    "react-native",
    "todo"
  ],
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider expo start --tunnel",
    "android": "cross-env NODE_OPTIONS=--openssl-legacy-provider expo start --android --tunnel",
    "ios": "cross-env NODE_OPTIONS=--openssl-legacy-provider expo start --ios --tunnel",
    "web": "cross-env NODE_OPTIONS=--openssl-legacy-provider expo start --web --tunnel",
    "eject": "cross-env NODE_OPTIONS=--openssl-legacy-provider expo eject --tunnel"
  },
  "dependencies": {
    "@expo/ngrok": "4.1.0",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/native": "^6.0.6",
    "expo": "~43.0.2",
    "expo-linking": "^2.4.2",
    "expo-status-bar": "~1.1.0",
    "moti": "^0.16.1",
    "native-base": "^3.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-checkbox-reanimated": "^0.1.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.14.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "~3.8.0",
    "react-native-svg": "^12.1.1",
    "react-native-web": "0.17.1",
    "shortid": "^2.2.16",
    "styled-components": "^5.3.3",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/shortid": "^0.0.29",
    "cross-env": "^7.0.3",
    "prettier": "^2.4.1",
    "typescript": "~4.3.5"
  }
}
